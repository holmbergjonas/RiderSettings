<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.pyc;*.pyo;*.rbc;*.yarb;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc" />
    <extensionMap>
      <mapping pattern="_headers" type="AUTO_DETECTED" />
      <mapping pattern="integration-tests-yml" type="AUTO_DETECTED" />
      <mapping ext="snapshot" type="AUTO_DETECTED" />
      <mapping pattern=".dockerignore" type="PLAIN_TEXT" />
      <removed_mapping ext="template" approved="true" type="PLAIN_TEXT" />
    </extensionMap>
  </component>
</application>